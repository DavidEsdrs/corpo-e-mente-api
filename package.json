{
  "name": "schedule-api",
  "version": "0.1.0",
  "description": "A simple api used to schedule appointments",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:create": "ts-node-dev ./node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "schema:drop": "ts-node-dev ./node_modules/typeorm/cli.js schema:drop"
  },
  "keywords": [
    "schedule",
    "appointments",
    "api"
  ],
  "author": "DavidErds",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "ts-node-dev": "^1.1.8",
    "typeorm": "^0.2.38",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/uuid": "^8.3.1"
  }
}
